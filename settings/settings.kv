<SettingsScreen>:
    name: 'settings'
    BoxLayout:
        orientation: 'vertical'
        MDTopAppBar:
            title: "Settings"
            left_action_items: [["menu", lambda x: app.root.ids.nav_drawer.set_state("open")]]
            right_action_items: [["menu", lambda x: app.root.ids.right_nav_drawer.set_state("open")]]
        MDScreenManager:
            id: settings_manager
